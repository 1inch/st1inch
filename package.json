{
  "name": "@1inch/st1inch",
  "version": "2.0.0",
  "description": "St1inch",
  "repository": {
    "type": "git",
    "url": "git@github.com:1inch/st1inch.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf artifacts cache coverage contracts/hardhat-dependency-compiler",
    "deploy": "hardhat deploy --network",
    "coverage": "hardhat coverage",
    "docify": "npx solidity-utils-docify",
    "format": "yarn format-js && yarn format-sol",
    "format-js": "prettier '**/*.js' --write",
    "format-sol": "prettier '**/*.sol' --write",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:sol:fix": "solhint --max-warnings 0 \"contracts/**/*.sol\" --fix",
    "lint": "yarn lint:js && yarn lint:sol",
    "lint:fix": "yarn lint:js:fix && yarn lint:sol:fix",
    "test": "hardhat test --parallel",
    "test:ci": "hardhat test"
  },
  "dependencies": {
    "@1inch/erc20-pods": "0.0.16",
    "@1inch/solidity-utils": "2.2.6",
    "@openzeppelin/contracts": "4.8.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-etherscan": "3.1.2",
    "@nomiclabs/hardhat-ethers": "2.2.1",
    "@nomicfoundation/hardhat-chai-matchers": "1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "1.0.6",
    "chai": "4.3.7",
    "dotenv": "16.0.3",
    "eslint": "8.27.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.5.1",
    "eslint-plugin-promise": "6.1.1",
    "hardhat": "2.12.2",
    "hardhat-dependency-compiler": "1.1.3",
    "hardhat-deploy": "0.11.20",
    "hardhat-gas-reporter": "1.0.9",
    "prettier": "2.7.1",
    "prettier-plugin-solidity": "1.0.0-rc.1",
    "solc": "0.8.17",
    "solhint": "3.3.7",
    "solidity-coverage": "0.8.2",
    "solidity-docgen": "0.6.0-beta.30"
  }
}
